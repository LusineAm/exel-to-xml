{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/convert",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
} 